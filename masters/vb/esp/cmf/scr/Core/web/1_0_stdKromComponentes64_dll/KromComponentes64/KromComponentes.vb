    Public Function LimitarCaracteres(ByVal cadena_ As String,
                                        ByVal limite_ As Integer)

        Dim nuevaCadena_ As String = ""

        Dim tamanoCadena_ As Integer

        tamanoCadena_ = cadena_.Length

        If limite_ >= tamanoCadena_ Then

            limite_ = tamanoCadena_

            nuevaCadena_ = cadena_.Substring(0, limite_)

        Else

            nuevaCadena_ = cadena_.Substring(0, limite_) & "..."

        End If

        Return nuevaCadena_

    End Function

