    Public Shared Sub DescargaArchivo(ByVal rutaArchivo_ As String,
                                   ByVal forzarDescarga_ As Boolean,
                                   Optional ByVal tipoContenido_ As String = "",
                                   Optional ByVal confirmaExistencia_ As Boolean = True)

        If confirmaExistencia_ Then

            If Not File.Exists(rutaArchivo_) Then

                Exit Sub

            End If

        End If


        Dim informacionArchivo_ As FileInfo

        Dim posicionInicial_ As Long = 0, TamanioArchivo_ As Long, posicionFinal_ As Long

        informacionArchivo_ = New FileInfo(rutaArchivo_)
        TamanioArchivo_ = informacionArchivo_.Length
        posicionFinal_ = TamanioArchivo_

        HttpContext.Current.Response.Clear()
        HttpContext.Current.Response.ClearHeaders()
        HttpContext.Current.Response.ClearContent()

        Dim rutaCompleta_ As String = IO.Path.GetFullPath(rutaArchivo_)

        Dim nombreArchivo_ As String = IO.Path.GetFileName(rutaCompleta_)

        Dim extensionArchivo_ As String = IO.Path.GetExtension(rutaCompleta_)

        If Not IsDBNull(extensionArchivo_) Then

            extensionArchivo_ = LCase(extensionArchivo_)

        End If

        If tipoContenido_ = "" Then

            Select Case extensionArchivo_

                Case ".htm", ".html"
                    tipoContenido_ = "text/HTML"

                Case ".txt"
                    tipoContenido_ = "text/plain"

                Case ".doc", ".rtf"
                    tipoContenido_ = "Application/msword"

                Case ".csv", ".xls"
                    tipoContenido_ = "Application/x-msexcel"

                Case ".pdf"
                    tipoContenido_ = "Application/pdf"

                Case Else
                    tipoContenido_ = "text/plain"

            End Select

        End If

        If tipoContenido_ <> "" Then

            HttpContext.Current.Response.ContentType = tipoContenido_

        End If

        Dim Range As String = HttpContext.Current.Request.Headers("Range")

        If Not ((Range Is Nothing) Or (Range = "")) Then

            Dim StartEnd As Array = Range.Substring(Range.LastIndexOf("=") + 1).Split("-")

            If Not StartEnd(0) = "" Then
                posicionInicial_ = CType(StartEnd(0), Long)
            End If
            If StartEnd.GetUpperBound(0) >= 1 And Not StartEnd(1) = "" Then
                posicionFinal_ = CType(StartEnd(1), Long)
            Else
                posicionFinal_ = TamanioArchivo_ - posicionInicial_
            End If
            If posicionFinal_ > TamanioArchivo_ Then
                posicionFinal_ = TamanioArchivo_ - posicionInicial_
            End If

            HttpContext.Current.Response.StatusCode = 206

            HttpContext.Current.Response.StatusDescription = "Partial Content"

            HttpContext.Current.Response.AppendHeader("Content-Range", "bytes " & posicionInicial_ & "-" & posicionFinal_ & "/" & TamanioArchivo_)

        End If
        'HttpContext.Current.Response.AppendHeader("Content-disposition", "inline; target=" & "_blank")
        '--HttpContext.Current.Response.AppendHeader("Content -Disposition:", "attachment; filename=jaja.pdf")
        'If Not (tipo_ = "") And (posicionInicial_ = 0) Then
        '    HttpContext.Current.Response.ContentType = Type.
        'End If
        If (forzarDescarga_) Then ' Will cause a download dialog (not using this method for the PDFs) 

            HttpContext.Current.Response.AppendHeader("Content-disposition", "attachment; filename=" & informacionArchivo_.Name)

        End If


        HttpContext.Current.Response.WriteFile(rutaArchivo_, posicionInicial_, posicionFinal_)

        HttpContext.Current.Response.Flush()

        'HttpContext.Current.Response.Close()

        HttpContext.Current.Response.End()

        'End If

    End Sub

    Public Shared Sub DescargaReporte(ByVal o_Stream_ As Byte(), _
                                      ByVal t_NombreReporte_ As String, _
                                      ByVal i_TipoReporte_ As IReportw.Extensions)

        Dim t_TipoContenido_ As String = Nothing

        If Not o_Stream_ Is Nothing Then

            HttpContext.Current.Response.Clear()

            HttpContext.Current.Response.ClearHeaders()

            HttpContext.Current.Response.ClearContent()

            Select Case i_TipoReporte_

                Case IReportw.Extensions.xls

                    t_TipoContenido_ = "Application/x-msexcel"

                    t_NombreReporte_ = t_NombreReporte_ & ".xls"

                Case IReportw.Extensions.xlsx

                    t_TipoContenido_ = "Application/x-msexcel"

                    t_NombreReporte_ = t_NombreReporte_ & ".xlsx"

                Case IReportw.Extensions.csv

                    t_TipoContenido_ = "text/csv"

                    t_NombreReporte_ = t_NombreReporte_ & ".csv"

            End Select

            HttpContext.Current.Response.ContentType = t_TipoContenido_

            HttpContext.Current.Response.AppendHeader("Content-Disposition", String.Format("attachment;filename={0}", t_NombreReporte_))

            HttpContext.Current.Response.BinaryWrite(o_Stream_)

            HttpContext.Current.Response.Flush()

            'HttpContext.Current.Response.Close()

            HttpContext.Current.Response.End()

        End If

    End Sub

    Public Function ConvertirArchivoBase64(ByVal rutaArchivo_ As String) As String

        If Not File.Exists(rutaArchivo_) Then

            Exit Function

        End If

        Dim archivoBase64_ As String = Convert.ToBase64String(File.ReadAllBytes(rutaArchivo_))

        Return archivoBase64_

    End Function

